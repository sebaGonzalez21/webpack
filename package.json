{
  "name": "curso-de-webpack",
  "version": "1.0.0",
  "description": "este es un curso de webpack",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "webpack index.js bundle.js",
    "build:local": "webpack",
    "build:external": "webpack --config ./external/webpack.config.js",
    "build:css": "webpack --config ./css-style-loader/webpack.config.js",
    "build:extract:css": "webpack --config ./plugin-extract-text/webpack.config.js",
    "build:multi": "webpack --config ./multiples-entry-points/webpack.config.js",
    "build:dev": "webpack-dev-server --config ./webpack-dev-server/webpack.config.js",
    "build:babel": "webpack --config ./babel-loader/webpack.config.js",
    "build:images": "webpack --config ./url-loader-images/webpack.config.js",
    "build:fonts": "webpack --config ./url-loader-fonts/webpack.config.js",
    "build:video": "webpack --config ./file-loader-video/webpack.config.js",
    "build:json": "webpack --config ./json-loader/webpack.config.js",
    "build:react": "webpack --config ./react/webpack.config.js",
    "build:sass": "webpack --config ./sass-loader/webpack.config.js"
  },
  "keywords": [
    "webpack"
  ],
  "author": "Seba Gonzalez <seba_gonza_@hotmail.com> (https://www.facebook.com/SebastianGonzalez.21)",
  "license": "MIT",
  "devDependencies": {
    "babel-core": "6.26.0",
    "babel-loader": "7.1.4",
    "babel-preset-es2016": "6.24.1",
    "babel-preset-es2017": "6.24.1",
    "babel-preset-react": "6.24.1",
    "css-loader": "0.28.11",
    "extract-text-webpack-plugin": "3.0.2",
    "file-loader": "1.1.11",
    "json-loader": "0.5.7",
    "node-sass": "^4.13.1",
    "sass-loader": "6.0.7",
    "style-loader": "0.20.3",
    "url-loader": "1.0.1",
    "webpack": "3.8.1",
    "webpack-cli": "2.0.13",
    "webpack-dev-server": "2.1.0-beta.0"
  },
  "dependencies": {
    "react": "16.3.0",
    "react-dom": "16.3.0"
  }
}
